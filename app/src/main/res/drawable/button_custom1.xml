<selector xmlns:android="http://schemas.android.com/apk/res/android">
  <item android:state_pressed="true" >
    <shape>
      <solid
          android:color="#eedf3b" />
      <stroke
          android:width="3dp"
          android:color="#eedf3b" />
      <corners
          android:radius="12dp" />
      <padding
          android:left="10dp"
          android:top="10dp"
          android:right="10dp"
          android:bottom="10dp" />
    </shape>
  </item>
  <item>
    <shape>
      <gradient
          android:startColor="#fab007"
          android:endColor="#fab007"
          android:angle="270" />
      <stroke
          android:width="3dp"
          android:color="#fab007" />
      <corners
          android:radius="12dp" />
      <padding
          android:left="10dp"
          android:top="10dp"
          android:right="10dp"
          android:bottom="10dp" />
    </shape>
  </item>
</selector>
